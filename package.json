{
  "displayName": "Homebridge August Door Sense",
  "name": "homebridge-august-door-sense",
  "version": "1.0.1",
  "description": "Homebridge Plugin for August Door Sense.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/WLBQE/homebridge-august-door-sense"
  },
  "bugs": {
    "url": "https://github.com/WLBQE/homebridge-august-door-sense/issues"
  },
  "engines": {
    "node": ">=14.18.1",
    "homebridge": ">=1.3.5"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "test:watch": "mocha -r ts-node/register src/*.spec.ts --watch --watch-files src/**/*.ts"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "homebridge": "^1.3.5",
    "homebridge-config-ui-x": "^4.41.5",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "prepublish": "tsc"
}
